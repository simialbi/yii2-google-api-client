{
	"name": "simialbi/yii2-google-api-client",
	"description": "yii2 google api client component",
	"keywords": [
		"yii2",
		"google",
		"google api",
		"api"
	],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/simialbi/yii2-google-api-client/issues",
		"source": "https://github.com/simialbi/yii2-google-api-client"
	},
	"authors": [
		{
			"name": "Simon Karlen",
			"email": "simi.albi@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"require": {
		"yiisoft/yii2": "~2.0",
		"simialbi/yii2-simialbi-base": "*",
		"google/apiclient": "~2.0"
	},
	"require-dev": {
		"yiisoft/yii2-coding-standards": "~2.0",
		"phpunit/phpunit": "*"
	},
	"suggest": {
		"simialbi/yii2-widget-chart": "Allows to display analytics data as charts"
	},
	"config": {
		"process-timeout": 1800
	},
	"autoload": {
		"psr-4": {
			"simialbi\\yii2\\google\\": "src"
		}
	}
}